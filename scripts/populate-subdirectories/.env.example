# Copy this file to .env and fill in your values

# Webflow API Token (get from: Webflow Dashboard > Site Settings > Integrations > API Access)
WEBFLOW_API_TOKEN=your_webflow_api_token_here

# Anthropic API Key (get from: console.anthropic.com)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Webflow Collection IDs (find in Webflow CMS > Collection Settings > copy from URL)
WEBFLOW_SUBDIRECTORIES_COLLECTION_ID=your_subdirectories_collection_id
WEBFLOW_PROJECTS_COLLECTION_ID=64aa150f02bee661d503cf59
WEBFLOW_MEMBERS_COLLECTION_ID=your_members_collection_id

# Webflow Site ID
WEBFLOW_SITE_ID=6481b864324e32f8eb266e2f
